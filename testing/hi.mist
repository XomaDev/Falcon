// Iterative function to calculate the Nth Fibonacci number
func FibIterative(n) = {
  if (n <= 1) {
    n // Returns 0 for n=0, and 1 for n=1
  } else {
    local a = 0
    local b = 1
    local count = 2 // Start from the 2nd iteration (to calculate F(2))
    local result

    while (count <= n) {
      result = a + b
      a = b
      b = result
      count = count + 1
    }
    result // The last expression evaluated is returned
  }
}

// Example usage:
// Calculate the 8th Fibonacci number (F(8) = 21, assuming F(0)=0, F(1)=1)
println("FibIterative(8) = " _ FibIterative(8))

// Calculate the 10th Fibonacci number (F(10) = 55)
println("FibIterative(10) = " _ FibIterative(10))