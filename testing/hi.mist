// Function to solve the quadratic equation ax^2 + bx + c = 0
// Returns a list containing the real roots.
// Returns an empty list if there are no real roots (discriminant < 0).
func solveQuadratic(a, b, c) = {
  // Calculate the discriminant: b^2 - 4ac
  local discriminant = (b * b) - (4 * a * c)
  
  if (discriminant < 0) {
    // No real roots
    [] // Return an empty list
  } else {
    local sqrtDiscriminant = root(discriminant) // Using the built-in root() function
    local denominator = 2 * a
    
    if (discriminant == 0) {
      // One real root (repeated root)
      local root = -b / denominator
      [root] // Return a list with one element
    } else {
      // Two distinct real roots
      local root1 = (-b + sqrtDiscriminant) / denominator
      local root2 = (-b - sqrtDiscriminant) / denominator
      [root1, root2] // Return a list with two elements
    }
  }
}

